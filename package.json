{
	"name": "olympus",
	"displayName": "olympus",
	"publisher": "CameronKing",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CameronRKing/olympus.git"
	},
	"description": "Rapid AST manipulation of Vue components + immediate TailwindCSS interactions.",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "olympus.openMenu",
				"title": "Open next Olympus menu"
			}
		],
		"keybindings": [
			{
				"command": "olympus.openMenu",
				"key": "alt+o",
				"mac": "alt+o"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"chai": "^4.2.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"@babel/preset-env": "^7.1.6",
		"ast-types": "^0.13.3",
		"bufferutil": "^4.0.1",
		"jscodeshift": "^0.10.0",
		"kind-of": "^6.0.3",
		"posthtml": "^0.13.1",
		"socket.io": "^2.4.0",
		"tailwind-zephyr": "^0.1.0",
		"utf-8-validate": "^5.0.2",
		"vue-ast-editor": "^0.1.1",
		"vue-parser": "^1.1.6"
	}
}
